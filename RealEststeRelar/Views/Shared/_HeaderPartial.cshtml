@model PropQuery
@using RealEststeRelar.Models
<div class="left-address">
    <p>
        @Model.Address<br /> @Model.City, @Model.State @Model.Zip<br />Predicted Price: $@string.Format("{0:N}", Convert.ToDecimal(@Model.HouseWorthVal.valuePredicted)).ToString().TrimEnd('0').TrimEnd('.')
    </p>
</div>
<div class="logo">
    <a href="#"><img src="~/Content/images/logo.png" width="210" height="19" alt="" /></a>
</div>
<div class="right-date">
    <span>Report Date: @DateTime.UtcNow.ToString("dddd, MMMM dd, yyyy")</span>
    <span>Analysis Date: @Convert.ToDateTime(Model.AnalysisDate).ToString("dddd, MMMM dd, yyyy")</span>
    @*<span>Report Date: Monday, November 2, 2015</span>
    <span>Analysis Date: Monday, November 2, 2015</span>*@
</div>